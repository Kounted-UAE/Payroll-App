{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@lib/*": [
        "lib/*"
      ],
      "@public/*": [
        "public/*"
      ],
      "@/assets/*": [
        "public/assets/*"
      ],
      "@/backgrounds/*": [
        "public/backgrounds/*"
      ],
      "@/clients/*": [
        "public/clients/*"
      ],
      "@/fonts/*": [
        "public/fonts/*"
      ],
      "@icons/*": [
        "public/icons/*"
      ],
      "@images/*": [
        "public/images/*"
      ],       
      "@logos/*": [
        "public/logos/*"
      ],
      "@team/*": [
        "public/team/*"
      ],
      "@hooks/*": [
        "hooks/*"
      ],
      "@scripts/*": [
        "lib/scripts/*"
      ]
    },
    "noImplicitAny": false,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "allowJs": true,
    "strict": false,
    "allowImportingTsExtensions": false
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "components/admin/XeroTabSectionHeader.tsx "  ],
  "exclude": [
    "node_modules"
  ]
}


